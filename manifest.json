{
  "name": "GDocs Presentation Preview Switcher",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["tabs", "http://*/*", "https://*/*"],
  "description": "Switches the visibility of the sidebar",
  "page_action": {
    "default_icon": "icon_default.png",
    "default_title": "Toggle Preview Pane"
  },
  "background": {
    "scripts": ["background.js"]
  }
}
